{ 
  "name" : "Chrome-Extension: SwapperCE",
  "version" : "0.1",
  "permissions": ["activeTab", "storage", "webRequest", "webRequestBlocking", "<all_urls>"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js":      ["swap.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Swap all the content!"
  },
  "manifest_version" : 2
}